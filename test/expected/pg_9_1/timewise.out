CREATE OR REPLACE FUNCTION date_properties(d date) RETURNS setof text LANGUAGE python AS
$python$

def main(d):
	return ['date: ' + str(d)] + [
		': '.join((attname, str(getattr(d, attname))))
		for attname in [
			'millennium',
			'century',
			'decade',
			'year',
			'quarter',
			'week',
			'month',
			'day',
			'dayofweek',
			'dayofyear',
		]
	] + [
		'epoch: {:.3f}'.format(d.epoch)
	]
$python$;
